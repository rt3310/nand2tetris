/**
 * Or gate:
 * out = (((a == 1) || (b == 1))), 1, 0) 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=w1);
    Not(in=b, out=w2);
    Nand(a=w1, b=w2, out=out);

    // Nand(a=a, b=a, out=w1);
    // Nand(a=b, b=b, out=w2);
    // Nand(a=w1, b=w2, out=out);
}
